<Window x:Class="ItogProjectOnEF.adminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ItogProjectOnEF"
        mc:Ignorable="d"
        Title="adminWindow" Height="450" Width="800">
    <Grid Background="Wheat">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="133*"/>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="208*"/>
            <ColumnDefinition Width="104*"/>
            <ColumnDefinition Width="98*"/>
            <ColumnDefinition Width="90*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="326*"/>
          
            <RowDefinition/>
        </Grid.RowDefinitions>


        <Button x:Name="Roles" Background="Orange" Content="Роли" Click="Roles_Click" Grid.ColumnSpan="3"    ></Button>
        <Button x:Name="Sotrudniki" Background="Orange" Content="Сотрудники" Click="Sotrudniki_Click" Grid.ColumnSpan="3"  Grid.Column="3" />
        
        
        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden" Grid.ColumnSpan="6" Grid.RowSpan="3" Grid.Row="1"/>
        
    </Grid>
</Window>
